# .npmignore
# Files and directories to ignore when publishing to npm

# Ignore development-specific files and directories
node_modules/
.git/
.gitignore
.vscode/
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*
.DS_Store
Thumbs.db

# Ignore test-related files
test/
*.test.js
*.spec.js

# Ignore documentation source files that aren't part of the published README
docs/
